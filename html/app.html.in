    <div id="login-page" class="hide login-page">
      <div class="login-container">
        <h1 class="esper-title">ESPER</h1>

        <form role="form">
          <div class="form-group">
            <label for="login-email" class="login-title">Email address</label>
            <input type="email" class="form-control" id="login-email">
          </div>
          <div class="form-group">
            <label for="login-password" class="login-title">Password</label>
            <input type="password" class="form-control" id="login-password">
          </div>
          <button id="login-button" class="btn btn-primary login-btn">
            LOG IN
          </button>
          <div>
            Forgot password?
            <a href="#!request-password">Click here to reset.</a>
          </div>
        </form>

        <div class="footer-push"></div>
      </div>
    </div>

    <div id="request-password-page" class="hide request-password-page">
      @@request-password.html@@
    </div>

    <div id="reset-password-page" class="hide reset-password-page">
      @@reset-password.html@@
    </div>

    <div id="home-page" class="hide home-page">
      <div id="all-tasks" class="hide">
        @@all-tasks.html@@
      </div>
      <div id="scheduling-task-list">
        @@sched-task-list.html@@
      </div>
      <div id="general-task-list" class="hide">
        @@gen-task-list.html@@
      </div>
      <div id="archive-task-list" class="hide">
        @@arch-task-list.html@@
      </div>
    </div>

    <div id="test-page" class="hide test-page">
      <div class="navbar-push container scrollable">
        <div id="test-content" class="row">
        </div>
      </div>
    </div>

    <div id="task-page" class="hide task-page">

      <div id="task-content" class="col-md-8 scrollable">

        <div id="task-navbar" class="navbar">
          <a href="#">
            <img class="svg menu" src="/assets/img/menu.svg">
          </a>
          <p id="task-title" class="navbar-text unselectable"></p>
          <a id="chat-sidebar">
            <img class="svg chat-icon" src="/assets/img/chat.svg">
          </a>
          <div id="select-task-type" class="hide"></div>
        </div>

        <div id="new-task">
          @@new-task.html@@
        </div>

        <div id="sched-task" class="hide">
          <div id="sched-step1-tab" class="hide">
            @@sched-step1-tab.html@@
          </div>
          <div id="sched-step2-tab" class="hide">
            @@sched-step2-tab.html@@
          </div>
          <div id="sched-step3-tab" class="hide">
            @@sched-step3-tab.html@@
          </div>
          <div id="sched-step4-tab" class="hide">
            @@sched-step4-tab.html@@
          </div>
        </div>

        <div id="gen-task" class="container task-container hide"></div>

      </div>

      <div id="chat" class="col-md-4 scrollable pull-right">
        <div id="chat-header">
          <ul class="nav nav-tabs chat-profile-tabs"></ul>
          <div class="chat-profile-details-div"></div>
        </div>
        <div class="tab-content chat-panel"></div>
      </div>

    </div>

    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/bootstrap.min.js"></script>
    <script src="/assets/js/can.min.js"></script>
    <script src="/assets/js/sha1.js"></script>
    <script src="/assets/js/jquery.timeago.js"></script>
    <script src="/assets/js/jquery.autosize.min.js"></script>

    <!-- API key activated by jeff@esper.co -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA1NI4SRYse6mchKKKUTYIvorssNcWju-g&sensor=false"></script>

    INCLUDES
  </body>
</html>
